{
   "default": true,
   "ul-indent": {
      "indent": 3,
      "start_indented": true
   },
   "no-multiple-blanks": { "maximum": 2 },
   "line-length": {
      "strict": true,
      "line_length": 90,
      "code_block_line_length": 160,
      "tables": false
   },
   "no-duplicate-heading": {
      "siblings_only": true
   },
   "hr-style": { "style": "---" },
   "code-block-style": { "style": "fenced" },
   "code-fence-style": { "style": "backtick" },
   "no-inline-html": false
}
